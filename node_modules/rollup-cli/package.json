  },
  "homepage": "https://github.com/ansenhuang/rollup-cli#readme",
  "devDependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "inquirer": "^3.0.6"
  },
  "engines": {
    "node": ">=5.0.0"
